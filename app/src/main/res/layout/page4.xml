<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            card_view:cardCornerRadius="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Types of Works"
                    android:textSize="20sp"
                    android:layout_margin="5dp"/>

                <RelativeLayout
                    android:id="@+id/relativeLayout1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/textview1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Flexible Denture"
                        android:textSize="15sp"
                        android:layout_margin="5dp"
                        android:layout_weight=".5"/>

                    <CheckBox
                        android:id="@+id/checkbox1a"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_alignBaseline="@+id/textview1"
                        android:layout_toLeftOf="@+id/checkbox1b"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:id="@+id/checkbox1b"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_alignBaseline="@+id/textview1"
                        android:layout_toLeftOf="@+id/checkbox1c"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:id="@+id/checkbox1c"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_alignBaseline="@+id/textview1"
                        android:layout_alignParentRight="true"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <TextView
                        android:id="@+id/textview2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Acrylic Denture"
                        android:layout_below="@+id/textview1"
                        android:textSize="15sp"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:id="@+id/checkbox2a"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBaseline="@+id/textview2"
                        android:layout_toLeftOf="@+id/checkbox2b"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:id="@+id/checkbox2b"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_alignBaseline="@+id/textview2"
                        android:layout_toLeftOf="@+id/checkbox2c"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:id="@+id/checkbox2c"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_alignBaseline="@+id/textview2"
                        android:layout_alignParentRight="true"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>


                    <TextView
                        android:id="@+id/textview3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Metal Framework"
                        android:layout_below="@+id/textview2"
                        android:textSize="15sp"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_alignBaseline="@+id/textview3"
                        android:layout_toLeftOf="@+id/checkbox3b"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_alignBaseline="@+id/textview3"
                        android:layout_alignParentRight="true"
                        android:layout_margin="5dp"/>

                    <TextView
                        android:id="@+id/textview4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Orthodontic Retainer"
                        android:layout_below="@+id/textview3"
                        android:textSize="15sp"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_alignBaseline="@+id/textview4"
                        android:layout_toLeftOf="@+id/checkbox4b"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_alignBaseline="@+id/textview4"
                        android:layout_alignParentRight="true"
                        android:layout_margin="5dp"/>

                    <TextView
                        android:id="@+id/textview5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Night Guard"
                        android:layout_below="@+id/textview4"
                        android:textSize="15sp"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_alignBaseline="@+id/textview5"
                        android:layout_toLeftOf="@+id/checkbox5b"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_alignBaseline="@+id/textview5"
                        android:layout_alignParentRight="true"
                        android:layout_margin="5dp"/>

                    <TextView
                        android:id="@+id/textview6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bite Block"
                        android:layout_below="@+id/textview5"
                        android:textSize="15sp"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_alignBaseline="@+id/textview6"
                        android:layout_toLeftOf="@+id/checkbox6b"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_alignBaseline="@+id/textview6"
                        android:layout_alignParentRight="true"
                        android:layout_margin="5dp"/>

                    <TextView
                        android:id="@+id/textview7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textview6"
                        android:text="Special Tray"
                        android:textSize="15sp"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_alignBaseline="@+id/textview7"
                        android:layout_toLeftOf="@+id/checkbox7b"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_alignBaseline="@+id/textview7"
                        android:layout_alignParentRight="true"
                        android:layout_margin="5dp"/>

                    <TextView
                        android:id="@+id/textview8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textview7"
                        android:text="Bite with Tray"
                        android:textSize="15sp"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_alignBaseline="@+id/textview8"
                        android:layout_toLeftOf="@+id/checkbox8b"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:id="@+id/checkbox8b"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_toLeftOf="@+id/checkbox8c"
                        android:layout_weight="1"
                        android:layout_margin="5dp"/>

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:layout_weight="1"
                        android:layout_alignBaseline="@+id/textview8"
                        android:layout_alignParentRight="true"
                        android:layout_margin="5dp"/>


                </RelativeLayout>

            </LinearLayout>

        </android.support.v7.widget.CardView>
    </ScrollView>

</RelativeLayout>